{
  "id": "zqy-formatAndSave",
  "name": "formatAndSave",
  "description": "1.prettier格式化（基于根目录配置文件），2.vue文件双分栏模式且自动折叠，3.html和css中嵌套注释，4.使用vscode打开所在目录，5.快速选中双引号区域，6.生成块注释`/** */`",
  "displayName": "formatAndSave",
  "version": "0.0.24",
  "publisher": "zqy",
  "engines": {
    "HBuilderX": "^2.7.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension",
  "configurationFileInfos": {
    ".prettierrc.js": {
      "description": "自定义prettier格式化规则。注意！被格式化的文件若存在于左侧目录项目列表下，优先使用项目下的prettier配置文件"
    }
  },
  "configurationFiles": [
    ".prettierrc.js"
  ],
  "activationEvents": [
    "onCommand:extension.foldAllExpandAndCopyEditor",
    "onCommand:extension.copyEditorAll",
    "onCommand:extension.contractScriptTag",
    "onCommand:extension.contractNoScriptTag",
    "onCommand:extension.contractStyleTag",
    "onCommand:extension.jumpLastQuote",
    "onCommand:extension.jumpNextQuote",
    "onCommand:extension.openWithVscode",
    "onCommand:extension.blockComment",
    "onCommand:extension.powerfulComment",
    "onCommand:extension.prettier"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.foldAllExpandAndCopyEditor",
        "title": "无折叠模式"
      },
      {
        "command": "extension.copyEditorAll",
        "title": "左分栏不折叠，右分栏则创建三次，分别显示template、script、style标签"
      },
      {
        "command": "extension.contractScriptTag",
        "title": "左分栏显示template、style标签，右分栏显示script标签"
      },
      {
        "command": "extension.contractNoScriptTag",
        "title": "左分栏显示script标签，右分栏显示template、style标签"
      },
      {
        "command": "extension.contractStyleTag",
        "title": "左分栏显示template、script标签，右分栏显示style标签"
      },
      {
        "command": "extension.jumpLastQuote",
        "title": "跳往上一个引号区域"
      },
      {
        "command": "extension.jumpNextQuote",
        "title": "跳往下一个引号区域"
      },
      {
        "command": "extension.openWithVscode",
        "title": "使用vscode打开所在目录"
      },
      {
        "command": "extension.blockComment",
        "title": "块注释/** */"
      },
      {
        "command": "extension.powerfulComment",
        "title": "嵌套注释"
      },
      {
        "command": "extension.prettier",
        "title": "prettier格式化代码"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.openWithVscode",
          "group": "openInExplorer",
          "title": "使用vscode打开所在目录",
          "when": "explorerResourceCount == 1 && ( explorerResourceIsFolder || explorerResourceIsWorkspaceFolder)"
        }
      ],
      "editor/context": [
        {
          "command": "extension.prettier",
          "group": "z_commands",
          "when": "editorTextFocus"
        },
        {
          "id": "copyEditor",
          "title": "向右复制分栏并自动折叠",
          "group": "z_commands"
        },
        {
          "command": "extension.foldAllExpandAndCopyEditor",
          "group": "copyEditor@1",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.copyEditorAll",
          "group": "copyEditor@1",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.contractScriptTag",
          "group": "copyEditor@1",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.contractNoScriptTag",
          "group": "copyEditor@1",
          "when": "editorTextFocus"
        },
        {
          "command": "extension.contractStyleTag",
          "group": "copyEditor@1",
          "when": "editorTextFocus"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.prettier",
        "key": "Ctrl+S",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.copyEditorAll",
        "key": "Ctrl+J",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.jumpLastQuote",
        "key": "Alt+Up",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.jumpNextQuote",
        "key": "Alt+Down",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.blockComment",
        "key": "Ctrl+Shift+/",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.powerfulComment",
        "key": "Ctrl+/",
        "when": "editorTextFocus"
      }
    ]
  },
  "extensionDependencies": [
    "plugin-manager"
  ],
  "keywords": [],
  "dependencies": {
    "await-to-js": "^3.0.0",
    "fast-diff": "^1.2.0"
  },
  "devDependencies": {
    "prettier": "2.8.7"
  },
  "license": "MIT"
}